<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/homenav"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/boardFragment"
        android:name="com.example.yuf2.Board.BoardFragment"
        android:label="fragment_board"
        tools:layout="@layout/fragment_board" >
        <action
            android:id="@+id/action_boardFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
        <action
            android:id="@+id/action_boardFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_boardFragment_to_friendFragment"
            app:destination="@id/friendFragment" />
        <action
            android:id="@+id/action_boardFragment_to_notiFragment"
            app:destination="@id/notiFragment" />
        <action
            android:id="@+id/action_boardFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
        <action
            android:id="@+id/action_boardFragment_to_settingFragment2"
            app:destination="@id/settingFragment" />
    </fragment>
    <fragment
        android:id="@+id/friendFragment"
        android:name="com.example.yuf2.Freind.FriendFragment"
        android:label="fragment_friend"
        tools:layout="@layout/fragment_friend" >
        <action
            android:id="@+id/action_friendFragment_to_notiFragment"
            app:destination="@id/notiFragment" />
