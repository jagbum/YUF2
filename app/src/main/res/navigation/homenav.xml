            android:id="@+id/action_chatFragment_to_notiFragment"
            app:destination="@id/notiFragment" />
        <action
            android:id="@+id/action_chatFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
        <action
            android:id="@+id/action_chatFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_chatFragment_to_friendFragment"
            app:destination="@id/friendFragment" />
        <action
            android:id="@+id/action_chatFragment_to_boardFragment"
            app:destination="@id/boardFragment" />
